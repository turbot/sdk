{
  "name": "@turbot/sdk",
  "author": "Turbot HQ, Inc",
  "description": "Turbot SDK",
  "version": "5.10.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/turbot/sdk.git"
  },
  "scripts": {
    "coverage": "npx nyc npm test",
    "test": "npx mocha"
  },
  "dependencies": {
    "@turbot/errors": "5.2.0",
    "@turbot/utils": "5.3.0",
    "async": "^3.2.0",
    "lodash": "^4.17.21",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.12.0",
    "mocha": "8.4.0",
    "nyc": "15.1.0",
    "prettier": "2.3.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended",
      "eslint:recommended",
      "plugin:you-dont-need-lodash-underscore/compatible"
    ],
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 10
    },
    "rules": {
      "no-console": "off",
      "you-dont-need-lodash-underscore/is-nil": "off"
    }
  },
  "prettier": {
    "printWidth": 120
  }
}
